}

.imagen-lado::before {
    content: "";
    position: absolute;
    top: -10px;
    left: 950px;
    width: calc(26% + 20px);
    height: calc(100% + 20px);
    box-shadow:
        inset -5px -5px rgba(255,255,255,.5),
        inset -10px -10px rgba(255,255,255,.4),
        inset -15px -15px rgba(255,255,255,.3),
        inset -20px -20px rgba(255,255,255,.2),
        inset -25px -25px rgba(255,255,255,.1),
        inset -30px -30px rgba(255,255,255,.05);
    animation-name: lightGlow;
    animation-duration: 2s;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
}

@keyframes lightGlow {
   from {
       box-shadow:
           inset -5px -5px rgba(0, 225, 255, 0.5),
           inset -10px -10px rgba(0, 217, 255, 0.459),
           inset -15px -15px rgba(0, 238, 255, 0.336),
           inset -20px -20px rgba(0, 217, 255, 0.315),
           inset -25px -25px rgba(0, 238, 255, 0.212),
           inset -30px -30px rgba(0, 238, 255, 0.041);
   }
   
   to {
       box-shadow:

       inset 5px 5px rgba(0, 238, 255, 0.5),
           inset 10px 10px rgba(0, 217, 255, 0.459),
           inset 15px 15px rgba(0, 247, 255, 0.336),
           inset 20px 20px rgba(0, 238, 255, 0.315),
